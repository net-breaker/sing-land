{
  "name": "singland",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start:ng": "wireit",
    "start:electron": "wireit",
    "build": "wireit"
  },
  "wireit": {
    "start:ng": {
      "command": "ng serve --port 8000"
    },
    "start:electron": {
      "command": "electron . --port 8000"
    },
    "build": {
      "command": "ng build --aot --base-href ./"
    }
  },
