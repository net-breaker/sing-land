    "@angular/router": "^15.1.0",
    "chokidar": "^3.5.3",
    "conf": "^10.2.0",
    "config": "^3.3.8",
    "dayjs": "^1.11.7",
    "electron-context-menu": "^3.6.1",
    "execa": "^6.1.0",
    "get-port": "^6.1.2",
    "js-yaml": "^4.1.0",
    "log4js": "^6.7.0",
